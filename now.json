{
  "redirects": [
    { "source": "/", "destination": "https://github.com/benborgers/glitch-proxy" }
  ],
  "rewrites": [
    {
      "source": "/(.*)/(.*)/(.*)/(.*)/(.*)/(.*)/(.*)/(.*)/(.*)",
      "destination": "https://$1.glitch.me/$2/$3/$4/$5/$6/$7/$8/$9"
    },
    {
      "source": "/(.*)/(.*)/(.*)/(.*)/(.*)/(.*)/(.*)/(.*)",
      "destination": "https://$1.glitch.me/$2/$3/$4/$5/$6/$7/$8"
    },
    {
      "source": "/(.*)/(.*)/(.*)/(.*)/(.*)/(.*)/(.*)",
      "destination": "https://$1.glitch.me/$2/$3/$4/$5/$6/$7"
    },
    {
      "source": "/(.*)/(.*)/(.*)/(.*)/(.*)/(.*)",
      "destination": "https://$1.glitch.me/$2/$3/$4/$5/$6"
    },
    {
      "source": "/(.*)/(.*)/(.*)/(.*)/(.*)",
      "destination": "https://$1.glitch.me/$2/$3/$4/$5"
    },
    {
      "source": "/(.*)/(.*)/(.*)/(.*)",
      "destination": "https://$1.glitch.me/$2/$3/$4"
    },
    {
      "source": "/(.*)/(.*)/(.*)",
      "destination": "https://$1.glitch.me/$2/$3"
    },
    {
      "source": "/(.*)/(.*)",
      "destination": "https://$1.glitch.me/$2"
    }
  ]
}